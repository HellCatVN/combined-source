{
  "compilerOptions": {
    "baseUrl": "./src",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "@api/*": ["./api/*"],
      "@assets/*": ["./assets/*"],
      "@bm/*": ["./bm/*"],
      "@components/*": ["./components/*"],
      "@configs/*": ["./configs/*"],
      "@constants/*": ["./constants/*"],
      "@context/*": ["./context/*"],
      "@enums/*": ["./enums/*"],
      "@hooks/*": ["./hooks/*"],
      "@interfaces/*": ["./interfaces/*"],
      "@layout/*": ["./layout/*"],
      "@locales/*": ["./locales/*"],
      "@pages/*": ["./pages/*"],
      "@stores/*": ["./stores/*"],
      "@utils/*": ["./utils/*"],
      "@validations/*": ["./validations/*"],
      "@api": ["./api"],
      "@configs": ["./configs"],
      "@constants": ["./constants"],
      "@context": ["./context"],
      "@enums": ["./enums"],
      "@interfaces": ["./interfaces"],
      "@locales": ["./locales"],
      "@stores": ["./stores"],
      "@utils": ["./utils"],
      "@validations": ["./validations"]
    }
  },
  "include": ["src"]
}
